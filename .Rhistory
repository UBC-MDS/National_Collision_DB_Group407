head(lung)
fit_km <-
survfit(
Surv(time, status) ~ 1,
data = lung)
model_weibull <- survreg(Surv(time, status) ~ 1, dist = "weibull", data = lung)
shape =  1/exp(tidy(model_weibull)$estimate[2])
scale = exp(tidy(model_weibull)$estimate[1])
surv <- tibble(x = seq(0, max(lung$time), 0.1),
Weibull = pweibull(x, shape = shape, scale = scale, lower.tail = FALSE))
surv <- surv %>%
rename(time = x,
surv = Weibull)
autoplot(fit_km) +
geom_line(aes(time, surv, color = 'Weibull'), data = surv, size = 1) +
labs(title = 'Survival function using KM estimator vs. with Weibull assumption', y = 'Survival', color = 'method')
mean <- scale * gamma(1 + 1/shape)
median <- scale * log(2)^(1/shape)
print(paste("Weibull Mean:", round(mean,3), "Median:", round(median,3)))
print(paste("KP Mean:", round(glance(fit_km)$rmean, 3), "Median:", round(glance(fit_km)$median, 3)))
